    <add key="DefaultQueryFile" value="~/default.rq"/>
    <add key="Stylesheet" value="/SPARQLEndpointDemo/sparql.css"/>
    <add key="LoadMode" value="PreloadAll"/>
    <add key="Reasoner1" value="VDS.RDF.Query.Inference.RdfsReasoner"/>
    <add key="/SPARQLEndpointDemo/sparql2/" value="AAT"/>
    <add key="AATDefaultGraph" value="http://nottm.ecs.soton.ac.uk/AllAboutThatWeb/profiles/"/>
    <add key="AATDefaultTimeout" value="30000"/>
    <add key="AATDefaultPartialResults" value="true"/>
    <add key="AATShowErrors" value="true"/>
    <add key="AATDBServer" value="localhost"/>
    <add key="AATDBName" value="aat_dev"/>
    <add key="AATDBUser" value="example"/>
    <add key="AATDBPassword" value="password"/>
    <add key="AATStylesheet" value="http://nottm.ecs.soton.ac.uk/AllAboutThatWeb/aat.css"/>
    <add key="/SPARQLEndpointDemo/bbc/" value="BBC"/>
    <add key="BBCDefaultTimeout" value="30000"/>
    <add key="BBCDefaultPartialResults" value="true"/>
    <add key="BBCShowErrors" value="true"/>
    <add key="BBCDBServer" value="localhost"/>
    <add key="BBCDBName" value="bbcone"/>
    <add key="BBCDBUser" value="example"/>
    <add key="BBCDBPassword" value="password"/>
    <add key="BBCLoadMode" value="PreloadAllAsync"/>
    <add key="BBCDefaultQueryFile" value="~/bbc.rq"/>
    <add key="VDS.RDF.Web.SQLResourceHandler" value="Res"/>
    <add key="ResDBServer" value="localhost"/>
    <add key="ResDBName" value="dotnetrdf_experimental"/>
    <add key="ResDBUser" value="example"/>
    <add key="ResDBPassword" value="password"/>
    <add key="ResLookupMode" value="Describe"/>
    <add key="ResRewriteRuleFind1" value=".*(vehicles/.*)"/>
    <add key="ResRewriteRuleReplace1" value="http://example.org/$1"/>
    <add key="/SPARQLEndpointDemo/dbpedia/" value="DBP"/>
    <add key="DBPEndpointURI" value="http://dbpedia.org/sparql"/>
    <add key="DBPSupportsTimeout" value="true"/>
    <add key="DBPDefaultTimeout" value="10000"/>
    <add key="DBPDefaultGraph" value="http://dbpedia.org"/>
    <add key="DBPDefaultQueryFile" value="~/dbpedia.rq"/>
    <add key="DBPStylesheet" value="/SPARQLEndpointDemo/sparql.css"/>
    <add key="DBPIntroText" value="~/dbpedia.txt"/>
    <add key="/SPARQLEndpointDemo/agraph" value="AG"/>
    <add key="AGEndpointURI" value="http://localhost:9875/catalogs/test/repositories/test"/>
    <add key="AGStylesheet" value="/SPARQLEndpointDemo/sparql.css"/>
    <add key="VDS.RDF.Web.DiskResourceHandler" value="Disk"/>
    <add key="DiskBaseURI" value="http://localhost/SPARQLEndpointDemo/resource2/"/>
    <add key="DiskDataFolder" value="~/App_Data"/>
    <add key="DiskDataExtension" value=".ttl"/>
    <add key="DiskDefaultFile" value="default"/>
    <add key="DiskAllowFormatTranslation" value="true"/>
    <add key="/SPARQLEndpointDemo/bbcdemo/" value="BBCDemo"/>
    <add key="BBCDemoShowErrors" value="true"/>
    <add key="BBCDemoDBServer" value="localhost"/>
    <add key="BBCDemoDBName" value="bbcdemo"/>
    <add key="BBCDemoDBUser" value="example"/>
    <add key="BBCDemoDBPassword" value="password"/>
    <add key="BBCDemoLoadMode" value="PreloadAllAsync"/>
    <add key="BBCDemoDefaultQueryFile" value="~/bbc.rq"/>
    <add key="/SPARQLEndpointDemo/virtuoso/" value="V"/>
    <add key="VStoreType" value="Virtuoso"/>
    <add key="VStoreServer" value="localhost"/>
    <add key="VStoreUser" value="example"/>
    <add key="VStorePassword" value="password"/>
    <add key="/SPARQLEndpointDemo/talis/" value="T"/>
    <add key="TStoreType" value="Talis"/>
    <add key="TStoreName" value="rvesse-dev1"/>
    <add key="/SPARQLEndpointDemo/4store" value="4"/>
    <add key="4StoreType" value="FourStore"/>
    <add key="4StoreServer" value="http://nottm-virtual.ecs.soton.ac.uk:8080/"/>
    <add key="4Stylesheet" value="/SPARQLEndpointDemo/sparql.css"/>
    <add key="/SPARQLEndpointDemo/agraph2" value="AG2"/>
    <add key="AG2StoreType" value="AllegroGraph"/>
    <add key="AG2StoreServer" value="http://localhost:9875/"/>
    <add key="AG2StoreCatalog" value="test"/>
    <add key="AG2StoreName" value="test"/>
    <add key="/SPARQLEndpointDemo/file/" value="File"/>
    <add key="FileDataFile" value="~/App_Data/default.ttl"/>
    <add key="/SPARQLEndpointDemo/caching/" value="Cac"/>
    <add key="CacDefaultGraph" value=""/>
    <add key="CacDefaultTimeout" value="30000"/>
    <add key="CacDefaultPartialResults" value="true"/>
    <add key="CacShowErrors" value="true"/>
    <add key="CacDBServer" value="localhost"/>
    <add key="CacDBName" value="write_test"/>
    <add key="CacDBUser" value="example"/>
    <add key="CacDBPassword" value="password"/>
    <add key="CacShowQueryForm" value="true"/>
    <add key="CacDefaultQueryFile" value="~/default.rq"/>
    <add key="CacStylesheet" value="/SPARQLEndpointDemo/sparql.css"/>
    <add key="CacLoadMode" value="PreloadAll"/>
    <add key="CacCacheDuration" value="0"/>
  </appSettings>
  <connectionStrings/>
  <system.web>
    <!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
    <compilation debug="true" targetFramework="4.0"/>
    <!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
    <authentication mode="Windows"/>
    <!--
            The <customErrors> section enables configuration 
            of what to do if/when an unhandled error occurs 
            during the execution of a request. Specifically, 
            it enables developers to configure html error pages 
            to be displayed in place of a error stack trace.

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
    <httpHandlers>
      <add verb="*" path="sparql/" validate="false" type="VDS.RDF.Web.SparqlHandler"/>
      <add verb="*" path="sparql2/" validate="false" type="VDS.RDF.Web.SparqlHandler"/>
      <add verb="*" path="resource/*" type="VDS.RDF.Web.SqlResourceHandler"/>
    </httpHandlers>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <!-- 
        The system.webServer section is required for running ASP.NET AJAX under Internet
        Information Services 7.0.  It is not necessary for previous version of IIS.
    -->
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <remove name="SPARQLHandler"/>
      <remove name="SPARQLHandler2"/>
      <remove name="SPARQLHandler3"/>
      <remove name="SPARQLHandler4"/>
      <remove name="SPARQLHandler5"/>
      <remove name="SPARQLHandler6"/>
      <remove name="SPARQLHandler7"/>
      <remove name="SPARQLHandler8"/>
      <remove name="SPARQLHandler9"/>
      <remove name="SPARQLHandler10"/>
      <remove name="SPARQLHandler11"/>
      <remove name="ResourceHandler"/>
      <remove name="ResourceHandler2"/>
      <remove name="RemoteSPARQLHandler"/>
      <add name="SPARQLHandler" verb="*" path="sparql/" type="VDS.RDF.Web.SparqlHandler"/>
      <add name="SPARQLHandler2" verb="*" path="sparql2/" type="VDS.RDF.Web.SparqlHandler"/>
      <add name="SPARQLHandler3" verb="*" path="bbc/" type="VDS.RDF.Web.SparqlHandler"/>
      <add name="SPARQLHandler4" verb="*" path="bbcdemo/" type="VDS.RDF.Web.SparqlHandler"/>
      <add name="SPARQLHandler5" verb="*" path="virtuoso/" type="VDS.RDF.Web.NativeSparqlHandler"/>
      <add name="SPARQLHandler6" verb="*" path="talis/" type="VDS.RDF.Web.NativeSparqlHandler"/>
      <add name="SPARQLHandler7" verb="*" path="file/" type="VDS.RDF.Web.FileSparqlHandler"/>
      <add name="SPARQLHandler8" verb="*" path="4store" type="VDS.RDF.Web.NativeSparqlHandler"/>
      <add name="SPARQLHandler9" verb="*" path="agraph" type="VDS.RDF.Web.RemoteSparqlHandler"/>
      <add name="SPARQLHandler10" verb="*" path="agraph2" type="VDS.RDF.Web.NativeSparqlHandler"/>
      <add name="SPARQLHandler11" verb="*" path="caching/" type="VDS.RDF.Web.SparqlHandler"/>
      <add name="ResourceHandler" verb="*" path="resource/*" type="VDS.RDF.Web.SqlResourceHandler"/>
      <add name="ResourceHandler2" verb="*" path="resource2/*" type="VDS.RDF.Web.DiskResourceHandler"/>
      <add name="RemoteSPARQLHandler" verb="*" path="dbpedia" type="VDS.RDF.Web.RemoteSparqlHandler"/>
    </handlers>
  </system.webServer>
</configuration>